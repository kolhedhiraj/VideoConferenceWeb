{% extends "VideoConferenceLogInBundle::layout.html.twig" %} {% block
content %}
<div>
	{% if is_granted('ROLE_USER') %} <img src={{ asset('apple-touch-icon.png') }}
		style="margin-left: 60px"> {% else %} <span
		style="color: white; margin-left: 60px">{{  'You must log in to see this content' | trans }}</span> {% endif %}
		<ul>
		{% for user in users %}
		<li>{{ loop.index }}. {{ user.firstName }} {{ user.lastName }}
		<ul>
		{% for room in user.rooms %}
		<li>{{ loop.index }}. {{ room.name }} {{ room.description }} </li>
		{% endfor %}
		</ul>
		</li>
		{% endfor %}
		</ul>
		<ul>
		{% for room in rooms %}
		<li>{{ loop.index }}. {{ room.name }} {{ room.owner }} {{ room.description }} {{ room.createdat|date }}</li>
		{% endfor %}
		</ul>
</div>

{% endblock %}
